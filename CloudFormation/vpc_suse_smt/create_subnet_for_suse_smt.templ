      "Condition" : "InfraSAP",
      "Properties" : {
        "RouteTableId" : { "Ref" : "PrivateRouteTable" },
        "DestinationCidrBlock" : { "Fn::Join" : [ "", [ { "Fn::FindInMap" : [ "RgnsrvSAP" ,  "us-west-2" , "Address" ] }, "/32" ] ] },
        "NatGatewayId" : { "Ref" : "VPCNatGateway" }
      }
